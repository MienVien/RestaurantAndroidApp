<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="icon.melb.melbicon">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <application
        android:allowBackup="true"
        android:screenOrientation="landscape"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:configChanges="orientation|keyboardHidden|screenSize">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainMenu"
            android:label="@string/title_activity_main_menu"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:theme="@style/AppTheme">
        </activity>
        <activity
            android:name=".WaitScreen"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_wait_screen">
        </activity>
        <activity
            android:name=".Payment"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_payment">
        </activity>
        <activity
            android:name=".Status"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_status_view">
        </activity>
        <activity android:name=".PaymentDetails"></activity>
        <meta-data
            android:name="io.fabric.ApiKey"
            android:value="ef0846c52627634c8fb82f063e57e8f2ff05fb36" />
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>